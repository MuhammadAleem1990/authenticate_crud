<h1>Add new student</h1>

<%= simple_form_for @student, :url => { :action => "create" } do |f| %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :course_name %>
    <%= f.input :grade %>
    <%= f.file_field :profile_image %><br><br>
    <%= f.button :submit %>

<% end %>
<%= link_to 'Back', {:action => 'list'} %>



